{
  "model": "deepseek-ai/DeepSeek-OCR",
  "max_new_tokens": 2048,
  "temp": 0.1,
  "max_side": 1920,
  "load_4bit": false,
  "prompt_path": "prompt.txt",
  "hotkey_ocr": "<ctrl>+<alt>+m",
  "hotkey_quit": "<ctrl>+<alt>+q",
  "use_global_hotkeys": true,
  "debug": false,
  "base_size": 1024,
  "image_size": 640,
  "crop_mode": true,
  "test_compress": true,
  "output_path": "ocr_output",
  "use_system_snipping": false,
  "use_flash_attention": false,
  "use_dollars_for_math": true
}